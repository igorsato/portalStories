
  <div class="mdl-card mdl-cell mdl-cell--4-col mdl-cell--4-col-tablet mdl-shadow--2dp" id="card_register">

    <div class="mdl-card__title">
        <h1 class="mdl-card__title-text">Cadastrar-se</h1>
    </div>
    <div class="mdl-card__supporting-text" id="card_text_register">
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name), class: "oxy-margin-bottom-80 oxy-color-light") do |f| %>
        <%= devise_error_messages! %>
        <div class="mdl-cell mdl-cell--12-col">
          <div class="mdl-textfield mdl-js-textfield textfield-demo mdl-textfield--floating-label">
              <%= f.label "E-mail", class: 'mdl-textfield__label' %>
              <%= f.email_field :email, autofocus: true, class: 'mdl-textfield__input' %>
          </div>
        </div>
        <div class="mdl-cell mdl-cell--12-col">              
          <div class="mdl-textfield mdl-js-textfield textfield-demo mdl-textfield--floating-label">
              <%= f.label :password, class: 'mdl-textfield__label' %>
              <%= f.password_field :password, autocomplete: "off", class: 'mdl-textfield__input' %>
          </div>
        </div>
        <div class="mdl-cell mdl-cell--12-col">
          <div class="mdl-textfield mdl-js-textfield textfield-demo mdl-textfield--floating-label">
              <%= f.label "Confirmar Senha", class: 'mdl-textfield__label' %><br />
              <%= f.password_field :password_confirmation, autocomplete: "off", class: 'mdl-textfield__input' %>
          </div>
          <div class="mdl-cell mdl-cell--12-col">
            <div class="mdl-textfield mdl-js-textfield textfield-demo mdl-textfield--floating-label">
            <% if @minimum_password_length %>
              <em>(Use no mÃ­nimo <%=@minimum_password_length%> caracteres )</em>
            <%end%>
          </div>
        </div>
        <div class="mdl-cell mdl-cell--6-col mdl-cell--middle">
          <%= f.submit "Cadastrar-se", class: 'mdl-button mdl-js-button mdl-js-ripple-effect mdl-button--raised mdl-js-ripple-effect mdl-button--accent oxy-margin-bottom-10' %>
        </div>
        <% end %>
    </div>
  </div>
  </div>
</div>