<p></p>
<%- if controller_name != 'sessions' %>
  <%= link_to "Entrar", new_session_path(resource_name), class: 'mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect'  %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <span><%= link_to "Cadastrar", new_registration_path(resource_name), class: 'mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect'  %></span>&nbsp;<br/>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <span><%= link_to "Esqueceu sua senha?", new_password_path(resource_name), class: 'mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect'  %></span>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Não recebeu instruções de confirmação?", new_confirmation_path(resource_name), class: 'mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect'  %><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Não recebeu instruções de desbloqueio?", new_unlock_path(resource_name), class: 'mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect'  %><br />
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Entrar com #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: 'mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect'  %><br />
  <% end -%>
<% end -%>
