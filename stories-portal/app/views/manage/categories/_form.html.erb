<div class="content">
	<div class="col-lg-12 form-group">	<%=link_to "Voltar", manage_categories_path, class: 'btn btn-primary' %>
</div>
	<div class="col-lg-12">
		<%= form_for([:manage, @category]) do |f|%>
					<!--<%= render partial: "layouts/partials/with_messages_of_errors", locals: {object: @category} %>-->
				<div class="col-lg-10 form-group">
					<%= f.label :name, "Nome da Categoria" %>
					<%= f.text_field :name, class: "form-control" %>
				</div>
				<div class="form-group col-lg-12">
					<%=f.submit "Salvar", class: 'btn btn-primary'%>
				</div>



		<%end%>
		</div>
</div>